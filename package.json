{
  "name": "html-sass-gulp-starter",
  "version": "3.0.0",
  "description": "An ES6-ready barebones HTML boilerplate running Sass and Gulp.",
  "repository": {
    "type": "git",
    "url": "git://github.com/MadeInHaus/html-boilerplate.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp",
    "build": "NODE_ENV=production && gulp production",
    "format": "prettier-eslint --logLevel debug --singleQuote --trailingComma es5 --tabWidth 4 --prettierLast --write \"src/**/*.{js,jsx}\" \"gulp/**/*.js\""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babelify": "10.0.0",
    "browser-sync": "2.24.7",
    "browserify": "16.2.3",
    "eslint": "5.6.0",
    "eslint-plugin-babel": "5.2.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "6.0.0",
    "gulp-changed": "3.2.0",
    "gulp-clean-css": "3.10.0",
    "gulp-filesize": "0.0.6",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "4.1.0",
    "gulp-notify": "3.2.0",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.1",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.1",
    "minimist": "1.2.0",
    "node-slack": "0.0.7",
    "prettier": "1.14.3",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "pretty-hrtime": "1.0.3",
    "require-dir": "1.0.0",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0"
  }
}
